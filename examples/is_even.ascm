MAIN $num $num_is_even
OUT "Input an integer: "
IN $num
OUT *$num
OUTL
OUT "Is "
OUT *$num
OUT " even? It is "
CALL @is_even *$num $num_is_even
CALLF *$num_is_even @print_even
SUB "1" *$num_is_even $num_is_even
CALLF *$num_is_even @print_odd

DEF @print_even
OUT "even!"

DEF @print_odd
OUT "odd!"

DEF @is_even $num $ret
MOD *$num "2" *$ret
EQ **$ret "0" *$ret
